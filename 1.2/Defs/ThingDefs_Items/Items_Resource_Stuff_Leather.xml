<Defs>


	<ThingDef ParentName="LeatherBase">
		<defName>RttR_FlightmareDragonScales</defName>
		<label>flightmare dragon scales</label>
		<description>Hard overlapping scales with a shiny metallic sheen. Exceptionally durable in combat.</description>
		<graphicData>
			<texPath>Things/Item/RttR_FlightmareScales</texPath>
			<graphicClass>Graphic_Single</graphicClass>

		</graphicData>
		 <stackLimit>2500</stackLimit>
		<statBases>
			<DeteriorationRate>0.5</DeteriorationRate>
			<MarketValue>1.5</MarketValue>
			<StuffPower_Armor_Sharp>1.75</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>1</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>1.65</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>28</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>26</StuffPower_Insulation_Heat>
			<Flammability>0</Flammability>
		</statBases>
		<stuffProps>
			<categories>
				<li>Metallic</li>
			</categories>
			<statFactors>
				
				<WorkToMake>3</WorkToMake>
				<WorkToBuild>3</WorkToBuild>
				<Beauty>3.5</Beauty>
				 <Flammability>0</Flammability>
				<DeteriorationRate>0.5</DeteriorationRate>
			</statFactors>
			<color>(154,212,230)</color>
			<commonality>0</commonality>
		</stuffProps>
		<smallVolume>true</smallVolume>
	</ThingDef>


