		<mods>
			<li>Genetic Rim</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>


			
			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesReptilian"]/ingredients/li/filter/thingDefs</xpath>
				<value>
					<li>Corpse_RttR_Fury</li>
					
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/RecipeDef[defName = "GR_ExtractGenesReptilian"]/fixedIngredientFilter/thingDefs</xpath>
				<value>
					<li>Corpse_RttR_Fury</li>
					
					
				</value>
			</li>	
			
			




		</operations>
		</match>
	</Operation>


</Patch>

